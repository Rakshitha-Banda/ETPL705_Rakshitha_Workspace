package com.evergent.corejava.exceptionhandling;

public class Nested {

	public static void main(String[] args) {
		int a=0;
		String s=null;
		try {
			System.out.println(10/a);
			try {
				System.out.println(s.length());
			}
			catch(NullPointerException e) {
				System.out.println(e);
			}
		}
		catch(Exception e) {
			System.out.println(e);
		}
				

	}

}
